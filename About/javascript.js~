function websites()
{
document.getElementById("websites").style.display="none";
document.getElementById("photography").style.display="none";
document.getElementById("seo").style.display="none";
document.getElementById("applications").style.display="none";

document.getElementById("websites").style.display="initial";
}

function photography()
{
document.getElementById("websites").style.display="none";
document.getElementById("photography").style.display="none";
document.getElementById("seo").style.display="none";
document.getElementById("applications").style.display="none";

document.getElementById("photography").style.display="initial";
}

function seo()
{
document.getElementById("websites").style.display="none";
document.getElementById("photography").style.display="none";
document.getElementById("seo").style.display="none";
document.getElementById("applications").style.display="none";

